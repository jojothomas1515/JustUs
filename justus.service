Unit]
Description=JustUs
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/JustUs/
EnvironmentFile=/home/ubuntu/JustUs/justus_env.env
ExecStart=/usr/bin/gunicorn  -k eventlet -w 1 -b 0.0.0.0:5000 --threads 30 app:app

Restart=always
[Install]
WantedBy=multi-user.target
